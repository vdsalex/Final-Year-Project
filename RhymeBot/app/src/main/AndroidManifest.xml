<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.alex.rhymebot">

    <application
        android:name=".Euterpe"
        android:allowBackup="true"
        android:icon="@drawable/iconita_euterpe_completa"
        android:label="@string/app_name"
        android:roundIcon="@drawable/iconita_euterpe_completa"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activities.MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.ConversationActivity"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:parentActivityName=".Activities.MenuActivity">
        </activity>
        <activity
            android:name=".Activities.ModsActivity"
            android:parentActivityName=".Activities.MenuActivity">
        </activity>
        <activity
            android:name=".Activities.SavedVersesActivity"
            android:parentActivityName=".Activities.MenuActivity">
        </activity>
        <activity
            android:name=".Activities.ModEminescuActivity">
        </activity>
        <activity
            android:name=".Activities.ModBacoviaActivity">
        </activity>
        <activity
            android:name=".Activities.ModStanescuActivity">
        </activity>
    </application>

</manifest>